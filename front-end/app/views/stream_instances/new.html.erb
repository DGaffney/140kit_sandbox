<h1>New stream_instance</h1>
<% form_for(@stream_instance) do |f| -%>
  <%= f.error_messages -%>
  <p>
    <%= f.label :instance_id %><br />
    <%= f.text_field :instance_id %>
  </p>
  <p>
    <%= f.label :hostname %><br />
    <%= f.text_field :hostname %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end -%>
<%= link_to 'Back', stream_instances_path %>